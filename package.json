  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwl/battle-subway-js/issues"
  },
  "homepage": "https://github.com/jwl/battle-subway-js#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.14.14",
    "chai": "^4.2.0",
    "eslint": "^5.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "prettier": "1.18.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
